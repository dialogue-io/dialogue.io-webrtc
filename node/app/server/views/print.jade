script(type='text/javascript')
	mixpanel.track('User list page', {'page name' : document.title, 'url' : window.location.pathname});
	mixpanel.track_pageview("/print.html");
div.container(style='margin:20px')
	table.table.table-bordered.table-striped
		thead
			tr
				th(style='width:40px') #
				th(style='width:180px') Name
				th(style='width:200px') Location
				th(style='width:180px') Username
				th Account Created
		tbody
		- for (var i = 0; i < accts.length; i++)
			tr
				td= i+1
				td= accts[i].name
				td= accts[i].country
				td= accts[i].user
				td= accts[i].date